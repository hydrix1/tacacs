#!/usr/local/sbin/tac_plus

id = spawnd {
	listen = { port = 4902 }
	spawn = {
		instances min = 1
		instances max = 100
	}
	background = no
}

id = tac_plus {
	debug = PACKET AUTHEN AUTHOR ALL

	access log = ../output/access_4902.log
	accounting log = ../output/acct_4902.log

        key = cisco

	group = group_4902 {
		service = shell {
			cmd = write { permit .* }
			cmd = copy { permit running-config }
			cmd = dir { permit .* }
		}
		service = junos-exec {
			set local-user-name = let_me_in
		}
	}

	user = cisco {
		password = clear cisco
		service = shell {
			default cmd = permit
		}
	}

	user = test_4902_a {
		password = clear A4902
		service = shell {
			cmd = exit { permit .* }
			cmd = enable { permit .* }
			cmd = terminal { permit length }
			cmd = write { permit .* }
			cmd = copy { permit running-config }
			cmd = dir { permit .* }
		}
		service = junos-exec {
			set local-user-name = let_me_in
		}
	}

	user = test_4902_b {
		password = permit
		service = shell {
			cmd = exit { permit .* }
			cmd = enable { permit .* }
			cmd = terminal { permit length }
			cmd = write { permit .* }
			cmd = delete { permit usr/test2 }
			cmd = dir { permit .* }
		}
		service = junos-exec {
			set local-user-name = let_me_in
		}
	}

	user = test_4902_c {
		password = deny
		service = shell {
			cmd = exit { permit .* }
			cmd = enable { permit .* }
			cmd = terminal { permit length }
			cmd = write { permit .* }
		}
	}

	user = test_4902_g {
		password = clear G4902
		member = group_4902
		service = shell {
			cmd = exit { permit .* }
			cmd = enable { permit .* }
			cmd = terminal { permit length }
		}
	}
}
